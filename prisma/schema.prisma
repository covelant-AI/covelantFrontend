generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id         Int     @id @default(autoincrement())
  firstName  String?
  lastName   String?
  email      String
  isCoache   Boolean @default(false)
  coached    Coache?  @relation(fields: [coachId], references: [id])
  coachId    Int?
}

model Coache {
  id       Int     @id @default(autoincrement())
  email    String
  team     String?
  players  Player[]  
}



